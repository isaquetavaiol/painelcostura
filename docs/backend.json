{
  "entities": {
    "User": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "User",
      "type": "object",
      "description": "Represents a user of the Atelier Insights application.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the user entity."
        },
        "email": {
          "type": "string",
          "description": "User's email address.",
          "format": "email"
        },
        "firstName": {
          "type": "string",
          "description": "User's first name."
        },
        "lastName": {
          "type": "string",
          "description": "User's last name."
        },
        "googleId": {
          "type": "string",
          "description": "User's Google ID, if they signed up with Google."
        },
        "createdAt": {
          "type": "string",
          "description": "Timestamp indicating when the user account was created.",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "email",
        "firstName",
        "lastName",
        "createdAt"
      ]
    },
    "Service": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Service",
      "type": "object",
      "description": "Represents a service offered by the Atelier, such as tailoring adjustments, dressmaking, or customizations.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the service entity."
        },
        "name": {
          "type": "string",
          "description": "Name of the service."
        },
        "description": {
          "type": "string",
          "description": "Detailed description of the service."
        },
        "price": {
          "type": "number",
          "description": "Price of the service."
        }
      },
      "required": [
        "id",
        "name",
        "price"
      ]
    },
    "Expense": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Expense",
      "type": "object",
      "description": "Represents an expense incurred by the Atelier, categorized by type (materials, rent, supplies).",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the expense entity."
        },
        "categoryId": {
          "type": "string",
          "description": "Reference to ExpenseCategory. (Relationship: ExpenseCategory 1:N Expense)"
        },
        "date": {
          "type": "string",
          "description": "Date when the expense was incurred.",
          "format": "date-time"
        },
        "amount": {
          "type": "number",
          "description": "Amount of the expense."
        },
        "description": {
          "type": "string",
          "description": "Detailed description of the expense."
        }
      },
      "required": [
        "id",
        "categoryId",
        "date",
        "amount"
      ]
    },
    "ExpenseCategory": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "ExpenseCategory",
      "type": "object",
      "description": "Represents a category of expenses, such as materials, rent, or supplies.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the expense category entity."
        },
        "name": {
          "type": "string",
          "description": "Name of the expense category."
        },
        "description": {
          "type": "string",
          "description": "Detailed description of the expense category."
        }
      },
      "required": [
        "id",
        "name"
      ]
    },
    "Revenue": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Revenue",
      "type": "object",
      "description": "Represents revenue generated by the Atelier from various services.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the revenue entity."
        },
        "serviceId": {
          "type": "string",
          "description": "Reference to Service. (Relationship: Service 1:N Revenue)"
        },
        "date": {
          "type": "string",
          "description": "Date when the revenue was generated.",
          "format": "date-time"
        },
        "amount": {
          "type": "number",
          "description": "Amount of revenue generated."
        },
        "description": {
          "type": "string",
          "description": "Detailed description of the revenue."
        }
      },
      "required": [
        "id",
        "serviceId",
        "date",
        "amount"
      ]
    },
    "Project": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Project",
      "type": "object",
      "description": "Represents a project undertaken by the Atelier, linking services, expenses, and revenue.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the project entity."
        },
        "name": {
          "type": "string",
          "description": "Name of the project."
        },
        "description": {
          "type": "string",
          "description": "Detailed description of the project."
        },
        "startDate": {
          "type": "string",
          "description": "Date when the project started.",
          "format": "date-time"
        },
        "endDate": {
          "type": "string",
          "description": "Date when the project ended.",
          "format": "date-time"
        },
        "serviceIds": {
          "type": "array",
          "description": "References to Services. (Relationship: Project N:N Service)",
          "items": {
            "type": "string"
          }
        },
        "expenseIds": {
          "type": "array",
          "description": "References to Expenses. (Relationship: Project N:N Expense)",
          "items": {
            "type": "string"
          }
        },
        "revenueIds": {
          "type": "array",
          "description": "References to Revenues. (Relationship: Project N:N Revenue)",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "id",
        "name",
        "startDate"
      ]
    },
    "Client": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Client",
      "type": "object",
      "description": "Represents a client of the Atelier.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the client entity."
        },
        "name": {
          "type": "string",
          "description": "Client's full name."
        },
        "phone": {
          "type": "string",
          "description": "Client's phone number."
        }
      },
      "required": [
        "id",
        "name"
      ]
    }
  },
  "auth": {
    "providers": [
      "password",
      "google.com"
    ]
  },
  "firestore": {
    "structure": []
  }
}
